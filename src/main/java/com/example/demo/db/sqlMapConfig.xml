<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <properties resource="com/example/demo/db/db.properties"/>
  <typeAliases>
  	<typeAlias type="com.example.demo.vo.AddressVO" alias="addressVO"/>
  	<typeAlias type="com.example.demo.vo.CartVO" alias="cartVO"/>
  	<typeAlias type="com.example.demo.vo.ReviewVO" alias="reviewVO"/>
  	<typeAlias type="com.example.demo.vo.MemberVO" alias="memberVO"/>
  	<typeAlias type="com.example.demo.vo.MyWishVO" alias="myWishVO"/>
  	<typeAlias type="com.example.demo.vo.MyReviewVO" alias="myreviewVO"/>
	<typeAlias type="com.example.demo.vo.OrderListVO" alias="orderListVO"/>
	<typeAlias type="com.example.demo.vo.OrdersVO" alias="ordersVO"/>
	<typeAlias type="com.example.demo.vo.OrdersProductVO" alias="ordersProductVO"/>
	<typeAlias type="com.example.demo.vo.OrderBillVO" alias="orderBillVO"/>
  	<typeAlias type="com.example.demo.vo.ProductVO" alias="productVO"/>
  	<typeAlias type="com.example.demo.vo.ReceiverVO" alias="receiverVO"/>
  	<typeAlias type="com.example.demo.vo.ChallengeVO" alias="challengeVO"/>
  	<typeAlias type="com.example.demo.vo.WishListVO" alias="wishListVO"/>
  	<typeAlias type="com.example.demo.vo.ChallengeListVO" alias="challengelistVO"/>
	<typeAlias type="com.example.demo.vo.Pro_add_optionVO" alias="pro_add_optionVO"/>
	<typeAlias type="com.example.demo.vo.PointVO" alias="pointVO"/>
	<typeAlias type="com.example.demo.vo.ChallengeUserVO" alias="challengeuserVO"/>
	<typeAlias type="com.example.demo.vo.CerBoardVO" alias="cerboardVO"/>
	<typeAlias type="com.example.demo.vo.QnaVO" alias="qnaVO"/>
	<typeAlias type="com.example.demo.vo.ImgVO" alias="imgVO"/>
	<typeAlias type="com.example.demo.vo.OneVO" alias="oneVO"/>
	<typeAlias type="com.example.demo.vo.ProQnaVO" alias="proqnaVO"/>
	<typeAlias type="com.example.demo.vo.CommunityVO" alias="communityVO"/>
	<typeAlias type="com.example.demo.vo.EasyToStartVO" alias="EasyToStartVO"/>
    <typeAlias type="com.example.demo.vo.NoticeVO" alias="noticeVO"/>
    <typeAlias type="com.example.demo.vo.LikeVO" alias="likeVO"/>
 
	
  </typeAliases>
  <environments default="development">  
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/addressMapper.xml"/>
     <mapper resource="com/example/demo/db/cartMapper.xml"/> 
     <mapper resource="com/example/demo/db/reviewMapper.xml"/> 
    <mapper resource="com/example/demo/db/productMapper.xml"/>
    <mapper resource="com/example/demo/db/myWishMapper.xml"/>
     <mapper resource="com/example/demo/db/myreviewMapper.xml"/> 
     <mapper resource="com/example/demo/db/memberMapper.xml"/> 
	<mapper resource="com/example/demo/db/orderListMapper.xml"/> 
	<mapper resource="com/example/demo/db/ordersMapper.xml"/> 
	<mapper resource="com/example/demo/db/ordersProductMapper.xml"/> 
  	<mapper resource="com/example/demo/db/pro_add_optionMapper.xml"/> 
  	<mapper resource="com/example/demo/db/pointMapper.xml"/> 
  	<mapper resource="com/example/demo/db/receiverMapper.xml"/> 
	<mapper resource="com/example/demo/db/challengeMapper.xml"/> 
	<mapper resource="com/example/demo/db/wishListMapper.xml"/> 
	<mapper resource="com/example/demo/db/challengelistMapper.xml"/>  
	<mapper resource="com/example/demo/db/challengeUserMapper.xml"/>  
	<mapper resource="com/example/demo/db/cerboardMapper.xml"/>  
	<mapper resource="com/example/demo/db/qnaMapper.xml"/>  
	<mapper resource="com/example/demo/db/oneMapper.xml"/>
	<mapper resource="com/example/demo/db/proQnaMapper.xml"/>
	<mapper resource="com/example/demo/db/communityMapper.xml"/>  
	<mapper resource="com/example/demo/db/easyToStartMapper.xml"/>
    <mapper resource="com/example/demo/db/noticeMapper.xml"/>
    <mapper resource="com/example/demo/db/imgMapper.xml"/>
 	<mapper resource="com/example/demo/db/likeMapper.xml"/>

  </mappers>
</configuration>